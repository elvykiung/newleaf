<div class="px-5">
  <h1>Welcome!</h1>

  <input type="text" id="search" placeholder="Search plants" />
  <script>
    $(document).ready(function () {
      $("#search").on('keyup', (e) => {
        //if (e.keyCode == 13) {
        var search = $(e.target).val()
        console.log("[search] autocomplete: " + search)
        $.ajax(`/api/plants/common_name/${search}`, function (data) {
          console.log("[search] autocomplete results for: " + search + " = " + data);
          $("#search").append(data)
        });
        //}
      });
    });
  </script>
</div>


